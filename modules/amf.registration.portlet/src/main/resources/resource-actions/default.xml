<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping
        PUBLIC "-//Liferay//DTD Resource Action Mapping 7.3.0//EN"
        "http://www.liferay.com/dtd/liferay-resource-action-mapping_7.3.0.dtd">

<resource-action-mapping>

    <portlet-resource>
        <portlet-name>com_amf_registration_portlet_AMFRegistrationPortlet</portlet-name>
        <permissions>
            <supports>
                <action-key>VIEW</action-key>
                <action-key>ADD</action-key>
                <action-key>EDIT</action-key>
                <action-key>DELETE</action-key>
            </supports>
            <site-member-defaults>
                <action-key>VIEW</action-key>
            </site-member-defaults>
            <guest-defaults>
                <action-key>VIEW</action-key>
            </guest-defaults>
            <guest-unsupported>
                <action-key>EDIT</action-key>
                <action-key>DELETE</action-key>
            </guest-unsupported>
        </permissions>
    </portlet-resource>

</resource-action-mapping>